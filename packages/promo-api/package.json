{
  "name": "@promo-protocol/api",
  "description": "DVMCP server that serves as the compute layer for The PROMO Protocol services",
  "module": "src/index.ts",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "dvmcp",
    "promo-protocol",
    "promo",
    "protocol",
    "api"
  ],
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./*": "./*.ts",
    "./*/*": "./*/*.ts"
  },
  "files": [
    "**/*.ts",
    "**/*.js",
    "!**/*.test.ts",
    "!**/*.test.js",
    "config.example.yml"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joinnextblock/promo-protocol.git"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "test": "bun test",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\""
  },
  "dependencies": {
    "@dvmcp/commons": "^0.1.9",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@promo-protocol/commons": "^0.0.2",
    "js-yaml": "^4.1.0",
    "pino": "^9.6.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "bun-types": "latest"
  }
}
